<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/pluto___margin_medium">

    <com.pluto.plugin.utilities.views.TabularDataView
        android:id="@+id/appDataTable"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent" />

    <com.pluto.plugin.utilities.views.TabularDataView
        android:id="@+id/deviceDataTable"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pluto___margin_medium"
        app:layout_constraintTop_toBottomOf="@+id/appDataTable" />

</androidx.constraintlayout.widget.ConstraintLayout>
